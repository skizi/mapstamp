
<% content_for :js_css_include do %>
	<script>
		<% if logged_in? %>
		var app = {
			loginFlag : true,
			provider : '<%= session[:provider] %>',
			user_id : <%= current_user.id %>
		}
		<% else %>
		var app = {
			loginFlag : false,
			user_id : -1
		}
		<% end %>
	</script>
	
<% end %>

	<div id="app">
	</div>
			

	<script type="text/javascript" src="/js/app.js"></script>


	<%= hidden_field_tag "authenticity_token", form_authenticity_token %>